<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.electricengineering.controllers.HistoryController"
            stylesheets="@../styles.css">  <!-- ← ДОБАВЛЕНО ЗДЕСЬ -->
    <children>
        <Label layoutX="320.0" layoutY="20.0" text="Calculation History" />
        <TabPane layoutX="20.0" layoutY="50.0" prefHeight="400.0" prefWidth="760.0">
            <tabs>
                <Tab text="Ohm's Law">
                    <content>
                        <TableView fx:id="ohmTable" prefHeight="200.0" prefWidth="200.0" />
                    </content>
                </Tab>
                <Tab text="Voltage Divider">
                    <content>
                        <TableView fx:id="dividerTable" prefHeight="200.0" prefWidth="200.0" />
                    </content>
                </Tab>
            </tabs>
        </TabPane>
        <Button layoutX="360.0" layoutY="460.0" mnemonicParsing="false" onAction="#back" text="Back" />
    </children>
</AnchorPane>